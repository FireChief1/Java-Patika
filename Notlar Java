import java.util.Scanner;

public class Main {
    public static void main(String[] args)
    {
        int mat,fizik,kimya,turkce,tarih,muzik;

        //Scanner sınıfı tanımlandı.
        Scanner inp = new Scanner(System.in);

        //kullanıcıdan bilgi alma

        System.out.print("Matematik dersi notu :");
        mat = inp.nextInt();

        System.out.print("Fizik dersi notu :");
        fizik = inp.nextInt();

        System.out.print("Kimya dersi notu :");
        kimya = inp.nextInt();

        System.out.print("Türkçe dersi notu :");
        turkce = inp.nextInt();

        System.out.print("Tarih dersi notu :");
        tarih = inp.nextInt();

        System.out.print("Müzik dersi notu :");
        muzik = inp.nextInt();

        double sonuc = (mat+fizik+kimya+turkce+tarih+muzik)/6;
        System.out.println("Ortalama :" +sonuc);

        //ternany
        System.out.println(sonuc >= 60 ? "Sınıfı geçti":"Sınıfta Kaldı");


    }
}
